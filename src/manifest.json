{
  "name": "Print Preview",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Print preview for Github Wiki",

  "content_scripts": [
    {
      "js": [
        "javascripts/jquery.min.js",
        "javascripts/print-preview.js"
      ],
      "matches": [
        "https://github.com/*/*/wiki/*"
      ]
    }
  ]
}
